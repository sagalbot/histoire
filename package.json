  "type": "module",
  "devDependencies": {
    "@histoire/plugin-nuxt": "^0.17.8",
    "@histoire/plugin-vue": "^0.17.8",
    "@nuxtjs/tailwindcss": "^6.10.0",
    "cypress": "^13.5.1",
    "histoire": "^0.17.8",
    "nuxt": "^3.8.2",
    "start-server-and-test": "^1.15.4",
    "vue": "^3.3.8"
  }
}
